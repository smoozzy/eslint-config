{
  "name": "@smoozzy/eslint-config",
  "description": "Shareable config for Smoozzy projects",
  "version": "0.1.0",
  "author": "Smoozzy Code <smoozzy.code@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smoozzy/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smoozzy/eslint-config.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "eslint --color --debug ."
  },
  "dependencies": {
    "semver": "6.3.0"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "lint-staged": "8.2.0",
    "yorkie": "2.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 5.0.0 < 7.0.0"
  },
  "optionalDependencies": {
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-jest-formatting": "^1.0.0"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1",
    "yarn": ">= 1.6.0"
  },
  "type": "commonjs",
  "main": "src/index.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint .",
      "git add"
    ]
  }
}
